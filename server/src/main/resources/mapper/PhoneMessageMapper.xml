<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dreambreeze.server.mapper.PhoneMessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.dreambreeze.server.domain.PhoneMessage">
        <id column="message_id" property="messageId" />
        <result column="phone_num" property="phoneNum" />
        <result column="contents" property="contents" />
        <result column="user_id" property="userId" />
        <result column="create_by" property="createBy" />
        <result column="create_at" property="createAt" />
        <result column="update_at" property="updateAt" />
        <result column="delete_at" property="deleteAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        message_id, phone_num, contents, user_id, create_by, create_at, update_at, delete_at
    </sql>

</mapper>
