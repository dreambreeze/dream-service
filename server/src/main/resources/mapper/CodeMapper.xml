<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dreambreeze.server.mapper.CodeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.dreambreeze.server.domain.Code">
        <id column="code_id" property="codeId" />
        <result column="code_name" property="codeName" />
        <result column="code_url" property="codeUrl" />
        <result column="cover_img" property="coverImg" />
        <result column="sort" property="sort" />
        <result column="view_num" property="viewNum" />
        <result column="security" property="security" />
        <result column="create_by" property="createBy" />
        <result column="create_name" property="createName" />
        <result column="create_at" property="createAt" />
        <result column="update_at" property="updateAt" />
        <result column="delete_at" property="deleteAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        code_id, code_name, code_url, cover_img, sort, view_num, security, create_by, create_name, create_at, update_at, delete_at
    </sql>

</mapper>
